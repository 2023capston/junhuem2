<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yolijoli">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NotesPro">
        <activity
            android:name="com.example.yolijoli.coDetailsActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.example.yolijoli.coActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.example.yolijoli.settingActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name="com.example.yolijoli.NoteDetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.yolijoli.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.example.yolijoli.CreateAccountActivity"
            android:exported="false" />
        <activity
            android:name="com.example.yolijoli.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.yolijoli.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.yolijoli.UmProfile"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.example.yolijoli.START_UMPROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.yolijoli.Search"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.example.yolijoli.START_SEARCH" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.yolijoli.SearchList"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.example.yolijoli.START_SEARCHLIST" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>